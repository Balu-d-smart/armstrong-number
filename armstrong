#include <stdio.h>
int main()
{int n,r,temp,reverse=0;
printf("enter the number:");
scanf("%d",&n);
temp=n;
while(n>0)
{
    r=n%10;
    reverse=reverse+(r*r*r);
   n=n/10;
}
if(temp==reverse)
{printf("the number is armstrong number");}
else
printf("the number is not a armstrong number");
  return 0;
}
